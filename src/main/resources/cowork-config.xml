<?xml version="1.0" encoding="UTF-8"?>

<!--  
	DTD(Document Type Definition)
	- 작성한 문서가 어떤 문서인지에 대해서 정의해 놓은 것.
	- 하나의 시스템 내에서 사용할 XML 데이터의 구조를 정의하여 유효성을 점검 시 사용하는 태그.
-->

<!-- 마이바티스 문서에서 복사해옴. -->
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>

	<!-- 별칭 주는 태그 -->
	<typeAliases>
		<typeAlias type="com.team3.model.member.MemberDAO" alias="memberDAO"/>
		<typeAlias type="com.team3.model.member.MemberDTO" alias="memberDTO"/>
		<typeAlias type="com.team3.model.TeamDTO" alias="teamDTO"/>
	
		<!-- *************** 지수 *************** -->	
		<typeAlias type="com.team3.model.AddressDAO" alias="addressDao"/>
		<typeAlias type="com.team3.model.DepartmentDTO" alias="departmentDto"/>
		<typeAlias type="com.team3.model.CustomerDTO" alias="customerDto"/>
		<typeAlias type="com.team3.model.MessengerDAO" alias="messengerDao"/>
		<typeAlias type="com.team3.model.Chat_RoomDTO" alias="chatRoomDto"/>
		<typeAlias type="com.team3.model.Chat_MemberDTO" alias="chatMemberDto"/>
		<typeAlias type="com.team3.model.Chat_MessageDTO" alias="chatMessageDto"/>
		<typeAlias type="com.team3.model.Messenger_NotiDTO" alias="messengerNotiDto"/>
		
		
	
		<!-- *************** 현우 *************** -->
		<typeAlias type="com.team3.model.CalendarDAO" alias="caldao"/>
		<typeAlias type="com.team3.model.CalendarDTO" alias="caldto"/>
		<typeAlias type="com.team3.model.Calendar_TypeDTO" alias="calTypeDto"/>
	
		<!-- *************** 세건 *************** -->
		<typeAlias type="com.team3.model.ProjectsDAO" alias="projectsdao"/>
		<typeAlias type="com.team3.model.ProjectsDTO" alias="projectsdto"/>
		<typeAlias type="com.team3.model.Main_ProjectsDTO" alias="maindto"/>
		<typeAlias type="com.team3.model.Projects_statusDTO" alias="statusdto"/>
		<typeAlias type="com.team3.model.PageDTO" alias="pagedto"/>
		
		<!-- *************** 귀근 *************** -->
		<typeAlias type="com.team3.model.EmailDAO" alias="emailDao"/>
		<typeAlias type="com.team3.model.EmailDTO" alias="emailDto"/>
	</typeAliases>

</configuration>